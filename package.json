{
  "name": "fcm-city-app",
  "version": "1.0.2",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit --incremental false",
    "dev:clean": "rm -rf node_modules yarn.lock android ios .expo && yarn cache clean && yarn install",
    "dev:check": "prettier --w ./ && yarn typecheck && yarn lint --fix",
    "prepare": "husky",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "gen": "graphql-codegen",
    "gen:watch": "graphql-codegen --watch",
    "build:android": "eas build --platform android --profile development",
    "build:ios": "eas build --platform ios --profile development",
    "build:dev": "eas build --platform all --profile development",
    "build:apk": "eas build --platform android --profile development",
    "version:show": "node -e \"const pkg=require('./package.json');const app=require('./app.json');console.log('ðŸ“¦ Package version:', pkg.version);console.log('ðŸ“± App version:', app.expo.version);\"",
    "version:patch": "node -e \"const fs=require('fs');const pkg=JSON.parse(fs.readFileSync('package.json'));const app=JSON.parse(fs.readFileSync('app.json'));const [major,minor,patch]=pkg.version.split('.');const newVersion=major+'.'+minor+'.'+(parseInt(patch)+1);pkg.version=newVersion;app.expo.version=newVersion;fs.writeFileSync('package.json',JSON.stringify(pkg,null,2));fs.writeFileSync('app.json',JSON.stringify(app,null,2));console.log('âœ… Version bumped to '+newVersion);\"",
    "version:minor": "node -e \"const fs=require('fs');const pkg=JSON.parse(fs.readFileSync('package.json'));const app=JSON.parse(fs.readFileSync('app.json'));const [major,minor,patch]=pkg.version.split('.');const newVersion=major+'.'+(parseInt(minor)+1)+'.0';pkg.version=newVersion;app.expo.version=newVersion;fs.writeFileSync('package.json',JSON.stringify(pkg,null,2));fs.writeFileSync('app.json',JSON.stringify(app,null,2));console.log('âœ… Version bumped to '+newVersion);\"",
    "version:major": "node -e \"const fs=require('fs');const pkg=JSON.parse(fs.readFileSync('package.json'));const app=JSON.parse(fs.readFileSync('app.json'));const [major,minor,patch]=pkg.version.split('.');const newVersion=(parseInt(major)+1)+'.0.0';pkg.version=newVersion;app.expo.version=newVersion;fs.writeFileSync('package.json',JSON.stringify(pkg,null,2));fs.writeFileSync('app.json',JSON.stringify(app,null,2));console.log('âœ… Version bumped to '+newVersion);\""
  },
  "dependencies": {
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.24",
    "@reduxjs/toolkit": "^2.8.2",
    "expo": "~53.0.20",
    "expo-font": "~13.3.2",
    "expo-linear-gradient": "~14.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "~0.28.17",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-redux": "^9.2.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~13.0.0",
    "eas-cli": "^16.17.4",
    "eslint": "^9.32.0",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-compact": "^8.40.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn test",
      "yarn dev:check"
    ],
    "*.{json,md,css}": "prettier --write"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
